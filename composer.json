{
  "name": "gosuperscript/docker-image-pruner",
  "description": "Automated Docker Image pruning for github container registry and docker hub",
  "type": "project",
  "config": {
    "sort-packages": true
  },
  "license": "GPL-3.0-or-later",
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "adambrett/shell-wrapper": "dev-master",
    "bramus/monolog-colored-line-formatter": "~3.0",
    "guzzlehttp/guzzle": "^7.3",
    "kint-php/kint": "^3.3",
    "monolog/monolog": "^2.2",
    "nesbot/carbon": "^2.62",
    "spatie/emoji": "^2.3",
    "vanilla/garden-cli": "~2.0",
    "jimmiw/php-time-ago": "^3.2"
  },
  "authors": [
    {
      "name": "Matthew Baggett",
      "email": "matthew.baggett@gosuperscript.com"
    }
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "DockerImagePruner\\": "src/"
    }
  }
}
